{
    "extends": "airbnb",
    "rules": {
        "no-console": 0,
        "no-debugger": 'warn',
        "no-restricted-syntax": ["error", "FunctionExpression", "WithStatement"],
        "no-underscore-dangle": 0,
        "max-len": 0,
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "globals": {
        "$": true
    },
}
